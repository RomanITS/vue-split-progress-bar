{
  "name": "vue2-progress-bar",
  "description": "A progress bar component for Vue.js",
  "version": "1.0.1",
  "author": "Lars M Hejll, hejlllars@hotmail.com , softwarefun.no",
  "license": "MIT",
  "homepage": "https://github.com/RomanITS/vue2-progress-bar",
  "private": false,
  "main": "./dist/ProgressBar.common.js",
  "browser": "dist/ProgressBar.umd.js",
  "scripts": {
    "serve": "vue-cli-service serve ./src/main.js",
    "build": "vue-cli-service build --target lib --name ProgressBar src/components/index.js",
    "version": "npm run build && git add -A",
    "postversion": "git push -u --follow-tags origin master && npm publish"
  },
  "devDependencies": {
    "@vue/babel-preset-app": "^5.0.6",
    "@vue/cli-plugin-babel": "^5.0.4",
    "@vue/cli-service": "^5.0.4",
    "node-sass": "7.0.1",
    "sass-loader": "^13.0.0",
    "vue": "^2.6.14",
    "vue-template-compiler": "^2.6.14"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RomanITS/vue2-progress-bar.git"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 1 versions",
    "not ie <= 8"
  ],
  "keywords": [
    "vuejs",
    "vue.js",
    "progress",
    "progressbar",
    "progress-bar"
  ],
  "bugs": {
    "url": "https://github.com/RomanITS/vue2-progress-bar/issues"
  }
}
